<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:jaxws="http://cxf.apache.org/jaxws"
  xsi:schemaLocation="
  http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans.xsd
  http://cxf.apache.org/jaxws
  http://cxf.apache.org/schemas/jaxws.xsd">

	<!-- Import statement optional but often required if you're declaring 
		 other CXF beans in this file such as CXF's JMX MBeans -->
	<import resource="classpath:META-INF/cxf/cxf.xml"/>

	<jaxws:endpoint
		id="syntheticpopulationservice"
		implementor="edu.pitt.apollo.SyntheticPopulationServiceImpl"
		wsdlLocation="WEB-INF/wsdl/syntheticpopulationservice.wsdl"
		address="/syntheticpopulationservice">
	</jaxws:endpoint>
</beans>
