<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://types.apollo.pitt.edu/v2_0/"
	xmlns:tns="http://types.apollo.pitt.edu/v2_0/" elementFormDefault="qualified">
	<simpleType name="Probability">
		<restriction base="double">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>

	<simpleType name="Fraction">
		<restriction base="double">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>

	<simpleType name="PositiveDouble">
		<restriction base="double">
			<minExclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="NonNegativeDouble">
		<restriction base="double">
			<minInclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="SimulatorType">
		<restriction base="string">
			<enumeration value="agent-based" />
			<enumeration value="compartmental" />
		</restriction>
	</simpleType>

	<simpleType name="UnitOfTime">
		<restriction base="string">
			<enumeration value="year" />
			<enumeration value="month" />
			<enumeration value="day" />
			<enumeration value="hour" />
			<enumeration value="minute" />
			<enumeration value="second" />
			<enumeration value="millisecond" />
		</restriction>
	</simpleType>

	<simpleType name="MethodCallStatusEnum">
		<restriction base="string">
			<enumeration value="exiting" />
			<enumeration value="held" />
			<enumeration value="queued" />
			<enumeration value="running" />
			<enumeration value="moving" />
			<enumeration value="waiting" />
			<enumeration value="completed" />
			<enumeration value="failed" />
			<enumeration value="unauthorized" />
			<enumeration value="authentication_failure" />
		</restriction>
	</simpleType>

	<complexType name="MethodCallStatus">
		<sequence>
			<element name="status" type="tns:MethodCallStatusEnum"
				maxOccurs="1" minOccurs="1" />
			<element name="message" type="string" maxOccurs="1"
				minOccurs="1" />
		</sequence>
	</complexType>

	<simpleType name="Gender">
		<restriction base="string">
			<enumeration value="M" />
			<enumeration value="F" />
			<enumeration value="U" />
		</restriction>
	</simpleType>

	<complexType name="Authentication">
		<sequence>
			<element name="requesterId" type="token" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="requesterPassword" type="token" maxOccurs="1"
				minOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="VisualizationOptions">
		<sequence>
			<element name="runId" type="token" />
			<element name="location" type="token" />
			<element name="outputFormat" type="token" />
		</sequence>
	</complexType>

	<complexType name="RunVisualizationMessage">
		<sequence>
			<element name="visualizerIdentification" type="tns:SoftwareIdentification" />
			<element name="authentication" type="tns:Authentication" />
			<element name="visualizationOptions" type="tns:VisualizationOptions" />
		</sequence>
	</complexType>

	<simpleType name="RunIdentification">
		<restriction base="token" />
	</simpleType>

	<complexType name="ServiceRecord">
		<sequence>
			<element name="url" type="anyURI" />
			<element name="softwareIdentification" type="tns:SoftwareIdentification" />
		</sequence>
	</complexType>

	<complexType name="ServiceRegistrationRecord">
		<complexContent>
			<extension base="tns:ServiceRecord">
				<sequence>
					<element name="authentication" type="tns:Authentication" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="SimulatorTimeSpecification">
		<sequence>
			<element name="unitOfTimeForSimulatorTimeStep" type="tns:UnitOfTime"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="numberOfUnitsOfTimeInOneSimulatorTimeStep"
				maxOccurs="1" minOccurs="1" type="tns:PositiveDouble">
			</element>
			<element name="runLength" type="positiveInteger" maxOccurs="1"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="IndividualTreatmentControlStrategy">
		<complexContent>
			<extension base="tns:InfectiousDiseaseControlStrategy">
				<sequence>
					<element name="individualTreatment" type="tns:Treatment"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="delayFromSymptomsToTreatment" type="tns:Duration"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="treatmentSystemLogistics" type="tns:TreatmentSystemLogistics"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="targetPopulationsAndPrioritizations"
						type="tns:ControlStrategyTargetPopulationsAndPrioritization"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>




	<complexType name="Vaccination">
		<complexContent>
			<extension base="tns:Treatment">
				<sequence>
					<element name="vaccine" type="tns:Vaccine" maxOccurs="1"
						minOccurs="1" />
					<element name="vaccinationEfficacies" type="tns:VaccinationEfficacy"
						maxOccurs="unbounded" minOccurs="0" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="RingIndividualTreatmentControlStrategy">
		<complexContent>
			<extension base="tns:IndividualTreatmentControlStrategy">
				<sequence>
					<element name="ringDiameterInMeters" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Infection">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="pathogenTaxonID" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="hostTaxonID" type="tns:NcbiTaxonId"
						maxOccurs="1" minOccurs="1" />
					<element name="infectiousPeriodDuration" type="tns:Duration"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="infectiousnessProfile"
						type="tns:ContinuousParametricProbabilityDistribution" maxOccurs="1"
						minOccurs="0">
					</element>
					<element name="latentPeriodDuration" type="tns:Duration"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="infectionAcquisition" type="tns:InfectionAcquisition"
						maxOccurs="unbounded" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Contamination">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="pathogenTaxonID" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1" />
					<element name="abioticEcosystemElement" type="tns:AbioticEcosystemEnum"
						maxOccurs="1" minOccurs="1" />
					<element name="contaminationPeriod" type="tns:Duration"
						maxOccurs="1" minOccurs="1" />
					<element name="contaminationAcquisition" type="tns:ContaminationAcquisition"
						maxOccurs="unbounded" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensus">
		<complexContent>
			<extension base="tns:Census">
				<sequence>
					<element name="populationSpecies" type="tns:NcbiTaxonId"
						maxOccurs="1" minOccurs="1" />
					<element name="pathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1"></element>
					<element name="censusData"
						type="tns:PopulationInfectionAndImmunityCensusData" maxOccurs="1"
						minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensusData">
		<complexContent>
			<extension base="tns:CensusData">
				<sequence>
					<element name="censusDataCells"
						type="tns:PopulationInfectionAndImmunityCensusDataCell" maxOccurs="unbounded"
						minOccurs="1">
					</element>
					<element name="exceptionSubpopulations"
						type="tns:PopulationInfectionAndImmunityCensusData" maxOccurs="unbounded"
						minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationTreatmentCensusData">
		<complexContent>
			<extension base="tns:CensusData">
				<sequence>

					<element name="treatmentCensusDataCells" type="tns:PopulationTreatmentCensusDataCell"
						maxOccurs="unbounded" minOccurs="1">
					</element>
					<element name="exceptionSubpopulations" type="tns:PopulationTreatmentCensusData"
						maxOccurs="unbounded" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="UrlOutputResource">
		<sequence>
			<element name="description" type="string" />
			<element name="URL" type="anySimpleType" />
		</sequence>
	</complexType>

	<complexType name="ServiceResult">
		<sequence>
			<element name="description" type="string" />
			<element name="URL" type="anyURI" />
		</sequence>
	</complexType>


	<complexType name="SoftwareIdentification">
		<sequence>
			<element name="softwareDeveloper" type="token" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="softwareName" type="token" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="softwareVersion" type="token" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="softwareType" type="tns:ApolloSoftwareType"
				maxOccurs="1" minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<simpleType name="ApolloLocationCode">
		<restriction base="token">
			<pattern value="[A-Z0-9]{2,17}"></pattern>
		</restriction>
	</simpleType>

	<complexType name="RequesterIdentification">
		<sequence>
			<element name="requesterName" type="token" />
			<element name="requesterCompany" type="token" />
			<element name="requesterEmail" type="token" />
		</sequence>
	</complexType>

	<complexType name="Vaccine">
		<sequence>
			<element name="description" type="string" maxOccurs="1"
				minOccurs="1" />
			<element name="vaccineIdentifier" type="token" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="valence" type="positiveInteger" maxOccurs="unbounded"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="AgeGroupEfficacy">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategory" />
			<element name="vaccineEfficacy" type="tns:Fraction" />
		</sequence>
	</complexType>

	<complexType name="VaccineContraindications">
		<sequence>
			<element name="infants" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="preschoolers" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="schoolAgeChildren" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="youngAdults" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="olderAdults" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="elderly" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="pregnantAdultsRestricted" type="boolean"
				maxOccurs="1" minOccurs="0" />
		</sequence>
	</complexType>

	<complexType name="Antiviral">
		<sequence>
			<element name="description" type="string" maxOccurs="1"
				minOccurs="1" />
			<element name="antiviralId" type="token" maxOccurs="1"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="RunSimulationsMessage">
		<sequence>
			<element name="batchConfigurationFile" type="anyURI" />
			<element name="acceptCachedResults" type="boolean" />
			<element name="softwareIdentification" type="tns:SoftwareIdentification" />
		</sequence>
	</complexType>

	<complexType name="RunSimulationsResult">
		<sequence>
			<element name="completedFile" type="anyURI" />
			<element name="runId" type="tns:RunIdentification" />
		</sequence>
	</complexType>

	<complexType name="AntiviralTreatment">
		<complexContent>
			<extension base="tns:Treatment">
				<sequence>
					<element name="antiviral" type="tns:Antiviral" maxOccurs="1"
						minOccurs="1" />
					<element name="antiviralTreatmentEfficacy" type="tns:AntiviralTreatmentEfficacy" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ArrayDimensionsDefinition" />
	<complexType name="PopulationStrataDefinition">
		<complexContent>
			<extension base="tns:ArrayDimensionsDefinition">
				<sequence>
					<element name="ageRanges" type="tns:AgeRangeCategory"
						maxOccurs="unbounded" minOccurs="0">
					</element>
					<element name="genders" type="tns:Gender" maxOccurs="unbounded"
						minOccurs="0">
					</element>
					<element name="diseaseOutcomes" type="tns:DiseaseOutcome"
						maxOccurs="unbounded" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensusDataCell">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategory"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="gender" type="tns:Gender" maxOccurs="1"
				minOccurs="0">
			</element>
			<element name="infectionState" type="tns:InfectionState"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="fractionInInfectionState" type="tns:Fraction"
				maxOccurs="1" minOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="PopulationTreatmentCensusDataCell">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategory"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="gender" type="tns:Gender" maxOccurs="1"
				minOccurs="0"></element>
			<element name="treatmentState" type="tns:TreatmentState"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="fractionInInfectionState" type="tns:Fraction"
				maxOccurs="1" minOccurs="1" />
		</sequence>

	</complexType>

	<complexType name="PopulationStrataArray">
		<sequence>
			<element name="description" type="tns:PopulationStrataDefinition"
				maxOccurs="1" minOccurs="1" />
			<element name="values" type="tns:Fraction" maxOccurs="unbounded"
				minOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="TemporalArrayDimensionsDefinition">
		<complexContent>
			<extension base="tns:ArrayDimensionsDefinition">
				<sequence>
					<element name="time" type="nonNegativeInteger" maxOccurs="unbounded"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="InfectionState">
		<restriction base="string">
			<enumeration value="susceptible" />
			<enumeration value="exposed" />
			<enumeration value="infectious" />
			<enumeration value="recovered" />
			<enumeration value="immuneAndActsLikeImmune" />
			<enumeration value="immuneButActsLikeNotImmune" />
			<enumeration value="newly_sick" />
		</restriction>
	</simpleType>

	<simpleType name="TreatmentState">
		<restriction base="string">
			<enumeration value="treated" />
			<enumeration value="not_treated" />
		</restriction>
	</simpleType>

	<complexType name="AgeRangeCategory">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="unitOfTimeForLowerBound" type="tns:UnitOfTime"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="lowerBound" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1" />
					<element name="unitOfTimeForUpperBound" type="tns:UnitOfTime"
						maxOccurs="1" minOccurs="1"></element>
					<element name="upperBound" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="TimeSpanCategory">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="unitOfTime" type="tns:UnitOfTime" maxOccurs="1"
						minOccurs="1">
					</element>
					<element name="timeZeroReference" type="tns:timeScaleEnum"
						maxOccurs="1" minOccurs="1"></element>
					<element name="startOfTimeSpan" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1" />
					<element name="durationInTimeUnits" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="timeScaleEnum">
		<restriction base="string">
			<enumeration value="simulatorTimeScale" />
			<enumeration value="controlStrategyTimeScale" />
			<enumeration value="individualTreatmentTimeScale" />
		</restriction>
	</simpleType>

	<complexType name="FacilityCategory">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="facility" type="tns:FacilityEnum" maxOccurs="1"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GenderCategory">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="gender" type="tns:Gender" maxOccurs="1"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="SimulatorTimeRange">
		<sequence>
			<element name="lowerBound" type="nonNegativeInteger" />
			<element name="upperBound" type="nonNegativeInteger" />
		</sequence>
	</complexType>

	<complexType name="TargetPriorityPopulation">
		<sequence>
			<element name="label" type="token" maxOccurs="1" minOccurs="1" />
			<element name="fractionOfTargetPopulationToPrioritize" type="tns:Fraction"
				maxOccurs="1" minOccurs="1" />
			<element name="priority" type="nonNegativeInteger" maxOccurs="1"
				minOccurs="1">
			</element>
			<choice>
				<element name="targetPopulationDefinition" type="tns:PopulationStrataDefinition"
					maxOccurs="1" minOccurs="1">
				</element>
				<element name="targetPopulationDefinitionEnum" type="tns:TargetPopulationDefinitionEnum"
					maxOccurs="1" minOccurs="1">
				</element>
			</choice>
		</sequence>
	</complexType>

	<complexType name="ControlStrategyTargetPopulationsAndPrioritization">
		<sequence>
			<choice>
				<element name="controlStrategyTargetPopulationsAndPrioritization"
					type="tns:TargetPriorityPopulation" maxOccurs="unbounded"
					minOccurs="1">
				</element>
				<element name="controlStrategyNamedPrioritizationScheme"
					type="token" maxOccurs="1" minOccurs="1">
				</element>
			</choice>
		</sequence>
	</complexType>

	<complexType name="InfectiousDiseaseControlStrategy">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="description" type="string" maxOccurs="1"
						minOccurs="1" />
					<element name="controlStrategyStartTime" type="tns:ControlStrategyStartTime"
						maxOccurs="1" minOccurs="1" />
					<element name="controlStrategyResponseDelay" type="tns:Duration"
						maxOccurs="1" minOccurs="1" />
					<element name="controlStrategyCompliance" type="tns:ProbabilisticParameter"
						maxOccurs="1" minOccurs="1" />
					<element name="controlStrategyReactiveEndPointFraction"
						type="tns:Fraction" maxOccurs="1" minOccurs="0" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ReactiveTriggerDefinition">
		<sequence>
			<element name="reactiveControlStrategyTest" type="token"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="reactiveControlStrategyThreshold" type="tns:Fraction"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="newCaseDetection" type="tns:DiseaseSurveillanceCapability"
				maxOccurs="1" minOccurs="1">
			</element>

		</sequence>
	</complexType>

	<simpleType name="TargetPopulationDefinitionEnum">
		<restriction base="token">
			<enumeration value="household_member_of_infant" />
		</restriction>
	</simpleType>



	<simpleType name="ApolloSoftwareType">
		<restriction base="string">
			<enumeration value="simulator" />
			<enumeration value="visualizer" />
			<enumeration value="syntheticPopulationGenerator" />
		</restriction>
	</simpleType>

	<complexType name="RunAndSoftwareIdentification">
		<sequence>
			<element name="runId" type="tns:RunIdentification" />
			<element name="softwareId" type="tns:SoftwareIdentification" />
		</sequence>
	</complexType>

	<simpleType name="SchoolClosureTargetFacilities">
		<restriction base="token">
			<enumeration value="all" />
			<enumeration value="individual" />
		</restriction>
	</simpleType>

	<complexType name="VisualizerResult">
		<sequence>
			<element name="runId" type="tns:RunIdentification" />
			<element name="visualizerOutputResource" type="tns:UrlOutputResource"
				maxOccurs="unbounded" minOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="SyntheticPopulationGenerationResult">
		<sequence>
			<element name="runId" type="tns:RunIdentification" />
			<element name="syntheticPopulationOutputResource" type="tns:UrlOutputResource"
				maxOccurs="unbounded" minOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="DiscreteNonparametricProbabilityDistribution">
		<complexContent>
			<extension base="tns:UnconditionalProbabilityDistribution">
				<sequence>
					<element name="probabilityValuePairs" type="tns:ProbabilityValuePair"
						maxOccurs="unbounded" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="TreatmentContraindication">
		<sequence>
			<element name="description" type="string" maxOccurs="1"
				minOccurs="0" />
			<element name="simulatorReferencablePopulation" type="tns:PopulationStrataDefinition"
				maxOccurs="1" minOccurs="0" />
			<element name="fractionOfSimulatorReferencablePopulation"
				type="tns:Fraction" maxOccurs="1" minOccurs="0" />
			<element name="pregnancy" type="boolean" maxOccurs="1"
				minOccurs="0" />
		</sequence>
	</complexType>

	<complexType name="ConditionalProbabilityTable">
		<sequence>
			<element name="axisDefinitions" type="tns:ArrayDimensionsDefinition"
				maxOccurs="1" minOccurs="1" />
			<choice>
				<element name="pointProbabilities" type="tns:Probability"
					maxOccurs="unbounded" minOccurs="1" />
				<element name="parametricDistribution" type="tns:Probability"
					maxOccurs="unbounded" minOccurs="1" />
			</choice>
		</sequence>
	</complexType>

	<complexType name="Duration">
		<sequence>
			<element name="unitOfTime" type="tns:UnitOfTime" maxOccurs="1"
