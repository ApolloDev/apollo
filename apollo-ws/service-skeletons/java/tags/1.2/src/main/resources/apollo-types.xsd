<?xml version="1.0" encoding="UTF-8"?>

<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://types.apollo.pitt.edu/"
	xmlns:tns="http://types.apollo.pitt.edu/" elementFormDefault="qualified">



	<simpleType name="Probability">
		<restriction base="double">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>

	<simpleType name="Fraction">
		<restriction base="double">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>

	<complexType name="ProbabilityArray">
		<sequence>
			<element name="value" type="tns:Probability" maxOccurs="unbounded"
				minOccurs="0" />
		</sequence>
	</complexType>

	<simpleType name="PositiveDouble">
		<restriction base="double">
			<minExclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="NonNegativeDouble">
		<restriction base="double">
			<minInclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="TimeStepUnit">
		<restriction base="string">
			<enumeration value="year" />
			<enumeration value="month" />
			<enumeration value="day" />
			<enumeration value="hour" />
			<enumeration value="minute" />
			<enumeration value="second" />
			<enumeration value="millisecond" />
		</restriction>
	</simpleType>

	<simpleType name="RunStatusEnum">
		<restriction base="string">
			<enumeration value="exiting" />
			<enumeration value="held" />
			<enumeration value="queued" />
			<enumeration value="running" />
			<enumeration value="moving" />
			<enumeration value="waiting" />
			<enumeration value="completed" />
			<enumeration value="failed" />
			<enumeration value="unauthorized" />
			<enumeration value="authentication_failure" />
		</restriction>
	</simpleType>

	<complexType name="RunStatus">
		<sequence>
			<element name="status" type="tns:RunStatusEnum" />
			<element name="message" type="string" />
		</sequence>
	</complexType>

	<simpleType name="OldAgeRange">
		<restriction base="string">
			<enumeration value="decile0-9" />
			<enumeration value="decile10-19" />
			<enumeration value="decile20-29" />
			<enumeration value="decile30-39" />
			<enumeration value="decile40-49" />
			<enumeration value="decile50-59" />
			<enumeration value="decile60-69" />
			<enumeration value="decile70-79" />
			<enumeration value="decile80-89" />
			<enumeration value="decile90-99" />
			<enumeration value="decile100-109" />
			<enumeration value="decile110-119" />
			<enumeration value="decile120-129" />
			<enumeration value="preschoolers" />
			<enumeration value="schoolAgeChildren" />
			<enumeration value="youngAdults" />
			<enumeration value="olderAdults" />
			<enumeration value="elderly" />
		</restriction>
	</simpleType>

	<simpleType name="Gender">
		<restriction base="string">
			<enumeration value="M" />
			<enumeration value="F" />
			<enumeration value="U" />
		</restriction>
	</simpleType>

	<complexType name="SupportedPopulationLocation">
		<sequence>
			<element name="populationLocation" type="token" />
			<element name="populationLocationDescription" type="token" />
		</sequence>
	</complexType>

	<complexType name="Authentication">
		<sequence>
			<element name="requesterId" type="token" maxOccurs="1"
				minOccurs="1">
				<annotation>
					<documentation source="http://purl.obolibrary.org/obo/asv/ASV_00000006" />
				</annotation>
			</element>
			<element name="requesterPassword" type="token" maxOccurs="1"
				minOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="VisualizationOptions">
		<sequence>
			<element name="runId" type="token" />
			<element name="location" type="token" />
			<element name="outputFormat" type="token" />
		</sequence>
	</complexType>

	<complexType name="VisualizerConfiguration">
		<sequence>
			<element name="visualizerIdentification" type="tns:SoftwareIdentification" />

			<element name="authentication" type="tns:Authentication" />
			<element name="visualizationOptions" type="tns:VisualizationOptions" />
		</sequence>
	</complexType>

	<simpleType name="RunIdentification">
		<restriction base="token" />
	</simpleType>

	<complexType name="ServiceRecord">
		<sequence>
			<element name="url" type="anyURI" />
			<element name="softwareIdentification" type="tns:SoftwareIdentification" />
		</sequence>
	</complexType>

	<complexType name="ServiceRegistrationRecord">
		<complexContent>
			<extension base="tns:ServiceRecord">
				<sequence>
					<element name="authentication" type="tns:Authentication" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="SimulatorTimeSpecification">
		<sequence>
			<element name="timeStepUnit" type="tns:TimeStepUnit"
				maxOccurs="1" minOccurs="1">
				<annotation>
					<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000014" />
				</annotation>
			</element>

			<element name="timeStepValue" maxOccurs="1" minOccurs="1"
				type="tns:PositiveDouble">
				<annotation>
					<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000012" />
				</annotation>
			</element>

			<element name="runLength" type="positiveInteger" maxOccurs="1"
				minOccurs="1">
				<annotation>
					<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000015" />
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="AntiviralTreatmentControlMeasure">
		<complexContent>
			<extension base="tns:ControlMeasure">
				<sequence>
					<element name="antiviralTreatment" type="tns:AntiviralTreatment"
						maxOccurs="1" minOccurs="1">
						<annotation>
							<documentation source="none" />
						</annotation>
					</element>
					<element name="antiviralSupplySchedule" type="int"
						maxOccurs="unbounded" minOccurs="1">
						<annotation>
							<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000051"/>
						</annotation>
					</element>
					<element name="antiviralTreatmentAdministrationCapacity"
						type="nonNegativeInteger" maxOccurs="unbounded" minOccurs="1">
						<annotation>
							<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000052"/>	
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
		<complexType name="Vaccination">
		<complexContent>
			<extension base="tns:Treatment">
				<sequence>
					<element name="description" type="token"
						maxOccurs="1" minOccurs="1" />

					<element name="vaccine" type="tns:Vaccine"
						maxOccurs="1" minOccurs="1" />
					<element name="vaccinationEfficacies" type="tns:VaccinationEfficacy" maxOccurs="unbounded" minOccurs="0"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="VaccinationControlMeasure">
		<complexContent>
			<extension base="tns:ControlMeasure">
				<sequence>
					<element name="vaccination" type="tns:Vaccination"
						maxOccurs="1" minOccurs="1">
						<annotation>
							<documentation source="none"/>
							
						</annotation>
					</element>
					<element name="vaccinationReactiveEndPointFraction" type="tns:Fraction"
						maxOccurs="1" minOccurs="1">
						<annotation>
							<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000150"/>
							
						</annotation>
					</element>
					<element name="vaccineSupplySchedule" type="int" maxOccurs="unbounded"
						minOccurs="1">
						<annotation>
							<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000046"/>
							
						</annotation>
					</element>
					<element name="vaccinationAdministrationCapacity" type="nonNegativeInteger"
						maxOccurs="unbounded" minOccurs="1">
						<annotation>
							<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000047"/>
							
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="PathogenicOrganism">
		<sequence>
			<element name="strainName" type="token" maxOccurs="1"
