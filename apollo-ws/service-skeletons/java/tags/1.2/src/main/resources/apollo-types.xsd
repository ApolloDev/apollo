				<annotation>
					<documentation source="none"/>
					
				</annotation>
			</element>
			<element name="ageRange" type="tns:AgeRange" maxOccurs="1"
				minOccurs="0">
				<annotation>
					<documentation source="none"/>
					
				</annotation>
			</element>
			<choice>
				<element name="populationDiseaseState" type="tns:PopulationDiseaseState"
					maxOccurs="unbounded" minOccurs="1">
