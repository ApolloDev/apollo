				</annotation>
			</element>

			<element name="latentPeriod" type="tns:DiseaseParameter"
				maxOccurs="1" minOccurs="1">
				<annotation>
					<documentation
						source="http://purl.obolibrary.org/obo/APOLLO_SV_00000017"/>
					
				</annotation>
			</element>

			<element name="reproductionNumber" type="tns:DiseaseParameter"
				maxOccurs="1" minOccurs="1">
				<annotation>
					<documentation
						source="http://purl.obolibrary.org/obo/APOLLO_SV_00000043"/>
					
				</annotation>
			</element>

			<element name="asymptomaticInfectionFraction" type="tns:Probability"
				maxOccurs="1" minOccurs="1">
				<annotation>
					<documentation source="http://purl.obolibrary.org/obo/APOLLO_SV_00000045"/>
					
				</annotation>
			</element>
		</sequence>
	</complexType>


	<complexType name="PopulationDiseaseState">
