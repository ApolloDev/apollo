<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://types.apollo.pitt.edu/"
	xmlns:tns="http://types.apollo.pitt.edu/"
	elementFormDefault="qualified">


    <complexType name="simulated_population">
    	<sequence>
    		<element name="location" type="string" maxOccurs="1"
    			minOccurs="1">
    		</element>
    		<element name="name" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
    		<element name="age" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
    		<element name="gender" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
    		<choice><element name="count" type="double" maxOccurs="1" minOccurs="1"></element><element name="subpopulation" type="tns:simulated_population" maxOccurs="unbounded" minOccurs="1">
    		</element></choice>
    	</sequence>
    </complexType>

    <element name="population" type="tns:simulated_population"></element>
</schema>