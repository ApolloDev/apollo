<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://types.apollo.pitt.edu/"
	xmlns:tns="http://types.apollo.pitt.edu/" elementFormDefault="qualified">

    <include schemaLocation="base-types/visualizer-identification.xsd"></include>
    <include schemaLocation="input-types/simulator-time-specification.xsd"></include>
	<include schemaLocation="input-types/population.xsd"></include>
	<include schemaLocation="input-types/authentication.xsd"></include>
	<include schemaLocation="input-types/disease.xsd"></include>
	<include schemaLocation="input-types/antiviral-control-measure.xsd"></include>
	<include schemaLocation="input-types/vaccination-control-measure.xsd"></include>
	<include schemaLocation="base-types/simulator-identification.xsd"></include>
	<include schemaLocation="base-types/base-types.xsd"></include>

	<complexType name="ServiceRegistrationRecord">
		<sequence>

			<element name="authentication" type="tns:Authentication"></element>
			<choice><element name="visualizerIdentification" type="tns:VisualizerIdentification">
			</element><element name="simulatorIdentification" type="tns:SimulatorIdentification">
			</element></choice>
            <element name="url" type="anyURI"></element>
        </sequence>
	</complexType>








	<complexType name="ServiceRecord">
		<sequence>

			<choice><element name="simulatorIdentification" type="tns:SimulatorIdentification">
			</element><element name="visualizerIdentification" type="tns:VisualizerIdentification">
			</element></choice>
		</sequence>
	</complexType>
</schema>