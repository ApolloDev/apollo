<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://types.apollo.pitt.edu/"
	xmlns:tns="http://types.apollo.pitt.edu/"
	elementFormDefault="qualified">

	<complexType name="SimulatorIdentification">
		<sequence>
			<element name="simulatorDeveloper" maxOccurs="1"
				minOccurs="1" type="token">
				<annotation>
					<documentation
						source="http://purl.obolibrary.org/obo/APOLLO_SV_00000009">
					</documentation>
				</annotation>
			</element>

			<element name="simulatorName" maxOccurs="1" minOccurs="1" type="token">
				<annotation>
					<documentation
						source="http://purl.obolibrary.org/obo/APOLLO_SV_00000010">
					</documentation>
				</annotation>
			</element>


			<element name="simulatorVersion" maxOccurs="1"
				minOccurs="1" type="token">
				<annotation>
					<documentation
						source="http://purl.obolibrary.org/obo/APOLLO_SV_00000013">
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
</schema>