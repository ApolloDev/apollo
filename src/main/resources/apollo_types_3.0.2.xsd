	<!-- X. (individual) Treatment efficacy -->
	<complexType name="TreatmentEfficacy">
		<sequence>
			<element name="hostIdentifier" type="tns:NcbiTaxonId"
				maxOccurs="1" minOccurs="1" />
			<element name="strainIdentifier" type="tns:ApolloPathogenCode"
