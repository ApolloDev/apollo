					<element name="buildingAffinity" type="tns:Fraction"
						maxOccurs="1" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<!-- 8. INFECTION AND INFECTIOUS DISEASE -->
	<!-- -->

	<complexType name="lInfection">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="location" type="tns:ApolloLocationCode"
						maxOccurs="1" minOccurs="1" />
					<element name="timePeriod" type="tns:Interval" maxOccurs="1"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Infection">
		<complexContent>
			<extension base="tns:lInfection">
				<sequence>
					<element name="pathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="host" type="tns:NcbiTaxonId" maxOccurs="1"
						minOccurs="1" />
					<element name="infectiousDiseases" type="tns:InfectiousDisease"
						maxOccurs="unbounded" minOccurs="0">
					</element>

					<element name="infectionAcquisitionsFromInfectedHosts"
						type="tns:InfectionAcquisitionFromInfectedHost" maxOccurs="unbounded"
						minOccurs="0">
					</element>
					<element name="infectionAcquisitionsFromContaminatedThings"
						type="tns:InfectionAcquisitionFromContaminatedAbioticThing"
						maxOccurs="unbounded" minOccurs="0">
					</element>
					<element name="InfectionAcquisitionsFromColonizedHosts"
						type="tns:InfectionAcquisitionFromColonizedHost" maxOccurs="unbounded"
						minOccurs="0"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="ContaminationAcquisition">
		<sequence>
			<choice>
				<element name="infectiousHostSource" type="tns:NcbiTaxonId"
					maxOccurs="1" minOccurs="1" />
				<element name="contaminatedSource" type="token" maxOccurs="1"
					minOccurs="1" />
				<element name="colonizedHostSource" type="tns:NcbiTaxonId"
					maxOccurs="1" minOccurs="1"></element>
			</choice>
			<element name="transmissionProbability" type="tns:ProbabilisticParameter"
				maxOccurs="1" minOccurs="1" />
		</sequence>
	</complexType>


	<complexType name="lInfectiousDisease">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="coosalPathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1" />
					<element name="hoostSpecies" type="tns:NcbiTaxonId"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="InfectiousDisease">
		<complexContent>
			<extension base="tns:lInfectiousDisease">
				<sequence>
					<element name="disease" type="tns:SnomedId" maxOccurs="1"
						minOccurs="1" />
					<element name="speciesWithDisease" type="tns:NcbiTaxonId"
						maxOccurs="1" minOccurs="1" />
					<element name="causalPathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1" />
					<element name="incubationPeriod" type="tns:Duration"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="prodromalPeriod" type="tns:Duration"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="fulminantPeriod" type="tns:Duration"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="otherIntervals" type="tns:Interval"
						maxOccurs="unbounded" minOccurs="0">
					</element>
					<element name="diseaseOutcomesWithProbabilities" type="tns:DiseaseOutcomeWithProbability"
						maxOccurs="unbounded" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>









	<complexType name="InfectionAcquisitionFromContaminatedAbioticThing">
		<sequence>
			<element name="contaminatedAbioticThing" type="tns:AbioticThingEnum"
				maxOccurs="1" minOccurs="1" />
			<element name="transmissionProbability" type="tns:ProbabilisticParameter"
				maxOccurs="1" minOccurs="1" />
		</sequence>
	</complexType>


	<complexType name="InfectionAcquisitionFromColonizedHost">
		<sequence>
			<element name="colonizedHost" type="tns:NcbiTaxonId"
				maxOccurs="1" minOccurs="1" />
			<choice>
				<element name="transmissionProbability" type="tns:ProbabilisticParameter"
					maxOccurs="1" minOccurs="1" />
				<element name="beta" type="string" maxOccurs="1" minOccurs="1"></element>
			</choice>
		</sequence>
	</complexType>

	<complexType name="InfectionAcquisitionFromInfectedHost">
		<sequence>
			<element name="infectedHost" type="tns:NcbiTaxonId"
				maxOccurs="1" minOccurs="1" />
			<element name="latentPeriodDuration" type="tns:Duration"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="infectiousPeriodDuration" type="tns:Duration"
				maxOccurs="1" minOccurs="1">
			</element>
			<choice>
				<element name="basicReproductionNumbers" type="tns:ReproductionNumber"
					maxOccurs="unbounded" minOccurs="1" />
				<element name="beta" type="tns:Rate" maxOccurs="1"
					minOccurs="1" />
				<element name="transmissionProbabilities" type="tns:TransmissionProbability"
					maxOccurs="unbounded" minOccurs="1" />
				<!-- <element name="forGlobalEpidemicSimulator" type="tns:GesParametersForContactAndTransmission" 
					maxOccurs="1" minOccurs="1" /> -->
				<element name="innoculationRate" type="tns:Rate" maxOccurs="1"
					minOccurs="0"></element>
			</choice>
		</sequence>
	</complexType>

	<complexType name="ReproductionNumber">
		<sequence>
			<choice>
				<element name="exactValue" type="tns:NonNegativeDouble"
					maxOccurs="1" minOccurs="1">
				</element>
				<element name="uncertainValue" type="tns:ProbabilityDistribution"
					maxOccurs="1" minOccurs="1">
				</element>
			</choice>
			<element name="referenceId" type="nonNegativeInteger"
				maxOccurs="1" minOccurs="0">
			</element>
		</sequence>
	</complexType>



	<!---MODEL FOR lversions of types that we want to be freestanding 'library 
		types'- -->
	<complexType name="lContamination">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="XXX" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="Contamination">
