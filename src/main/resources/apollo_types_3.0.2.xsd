	<!-- 9. CENSUS TYPES -->

	<complexType name="Census">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="description" type="string" maxOccurs="1"
						minOccurs="1">
					</element>
					<element name="location" type="tns:Location" maxOccurs="1"
						minOccurs="1">
					</element>
					<element name="simulatorTime" type="int" maxOccurs="1"
						minOccurs="1">
					</element>
					<element name="referenceDate" type="dateTime" maxOccurs="1"
						minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="CensusData">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="date" type="dateTime" maxOccurs="1"
						minOccurs="0">
					</element>
					<element name="location" type="tns:Location" maxOccurs="1"
						minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensus">
		<complexContent>
			<extension base="tns:Census">
				<sequence>
					<element name="pathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="censusData"
						type="tns:PopulationInfectionAndImmunityCensusData" maxOccurs="1"
						minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensusData">
		<complexContent>
			<extension base="tns:CensusData">
				<sequence>
					<element name="censusDataCells"
						type="tns:PopulationInfectionAndImmunityCensusDataCell" maxOccurs="unbounded"
						minOccurs="1">
					</element>
					<element name="exceptionSubpopulations"
						type="tns:PopulationInfectionAndImmunityCensusData" maxOccurs="unbounded"
						minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensusDataCell">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategoryDefinition"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="gender" type="tns:GenderEnum" maxOccurs="1"
				minOccurs="0">
			</element>
			<choice>
				<element name="infectionState" type="tns:InfectionStateEnum"
					maxOccurs="1" minOccurs="1">
				</element>
				<element name="diseaseState" type="tns:DiseaseOutcomeEnum"
					maxOccurs="1" minOccurs="1">
				</element>
			</choice>
			<element name="fractionInState" type="tns:Fraction"
				maxOccurs="1" minOccurs="1" />
		</sequence>
	</complexType>

	<simpleType name="InfectionStateEnum">
		<restriction base="string">
			<enumeration value="susceptible" />
			<enumeration value="greaterThanOrEqualToId50Exposure" />
			<enumeration value="latent" />
			<enumeration value="infectious" />
			<enumeration value="recovered" />
			<enumeration value="immuneAndActsLikeImmune" />
			<enumeration value="immuneButActsLikeNotImmune" />
			<enumeration value="newly_sick" />
		</restriction>
	</simpleType>

	<simpleType name="DiseaseOutcomeEnum">
		<restriction base="string">
			<enumeration value="asymptomatic" />
			<enumeration value="symptomatic" />
			<enumeration value="severely symptomatic" />
			<enumeration value="fulminant" />
			<enumeration value="hospitalization" />
			<enumeration value="death" />
			<enumeration value="recovery" />
		</restriction>
	</simpleType>

	<complexType name="PopulationTreatmentCensusData">
		<complexContent>
			<extension base="tns:CensusData">
				<sequence>

					<element name="treatmentCensusDataCells" type="tns:PopulationTreatmentCensusDataCell"
						maxOccurs="unbounded" minOccurs="1">
					</element>
					<element name="exceptionSubpopulations" type="tns:PopulationTreatmentCensusData"
						maxOccurs="unbounded" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationTreatmentCensus">
		<complexContent>
			<extension base="tns:Census">
				<sequence>
					<element name="populationSpecies" type="tns:NcbiTaxonId"
						maxOccurs="1" minOccurs="1" />
					<element name="treatmentCensusData" type="tns:PopulationTreatmentCensusData"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationTreatmentCensusDataCell">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategoryDefinition"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="gender" type="tns:GenderEnum" maxOccurs="1"
				minOccurs="0">
			</element>
			<element name="treatmentState" type="tns:TreatmentStateEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="fractionInTreatmentState" type="tns:Fraction"
				maxOccurs="1" minOccurs="1" />
		</sequence>
	</complexType>

	<simpleType name="TreatmentStateEnum">
		<restriction base="string">
			<enumeration value="treated" />
			<enumeration value="not_treated" />
		</restriction>
	</simpleType>

	<complexType name="PopulationColonizationCensus"></complexType>

	<complexType name="ContaminatedThingCensus">
		<complexContent>
			<extension base="tns:Census">
				<sequence>
					<element name="kindOfThing" type="tns:AbioticThingEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="pathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1" />
					<element name="censusData" type="tns:ContaminatedThingCensusData"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ContaminatedThingCensusData">
		<complexContent>
			<extension base="tns:CensusData">
				<sequence>
					<element name="fractionContaminated" type="tns:Fraction"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="exceptionSublocations" type="tns:ContaminatedThingCensusData"
						maxOccurs="unbounded" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="AbioticThingCensus">
		<complexContent>
			<extension base="tns:Census">
				<sequence>
					<element name="abioticThing" type="tns:AbioticThingEnum"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="OvipositionSiteCensus">
		<complexContent>
			<extension base="tns:AbioticThingCensus">
				<sequence>
					<element name="breteauIndex" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="0" />
					<element name="carryingCapacityPerHouse" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="0" />
					<element name="heterogenousCarryingCapacity" type="boolean"
						maxOccurs="1" minOccurs="0" />
					<element name="ratioOfOutdoorToIndoorOvisites" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="0" />
					<element name="maximumRatioOfOutdoorCarryingCapacityToIndoor"
						type="tns:NonNegativeDouble" maxOccurs="1" minOccurs="0" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<!-- X. INFECTIOUS DISEASE CONTROL MEASURE types -->


	<complexType name="InfectiousDiseaseControlMeasure">
