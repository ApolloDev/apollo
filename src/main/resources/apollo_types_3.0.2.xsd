<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://types.apollo.pitt.edu/v3_0_2/"
	xmlns:tns="http://types.apollo.pitt.edu/v3_0_2/" elementFormDefault="qualified">

	<!-- The types in this file are organized into sections as follows: -->

	<!-- 1. The InfectiousDiseaseScenario type, which can be thought of as the 
		root of fairly deep and bushy tree that encodes all information required 
		to express the input of, at present, six epidemic simulators -->
	<!-- 2. Basic numeric types used in the tree -->
	<!-- 3. Types for representing a multidimensional array -->
	<!-- 4. Types for distance, duration, rate, and interval -->
	<!-- 5. Types for probability distributions -->
	<!-- 6. Types for representing summary statistics -->
	<!-- 7. Simple types for identifiers for species, drugs, vaccines... -->
	<!-- 8. Populations -->

	<!-- 9. Types for representing infections, contaminations, and colonization -->
	<!-- 9. Types for representing INFECTIOUS DISEASEs -->
	<!-- 9. CENSUS TYPES -->

	<!-- X. types for specifying SIMULATOR OUTPUT and storing simulator results -->
	<!-- X. types for specifying an openMalaria EXPERIMENT and storing results -->
	<!-- A. UNDER DEVELOPMENT decision analysis -->
	<!-- B. UNDERDEVELOPMENT Bayesian network -->
	<!-- C. DEVELOPMENT SUSPENDED -->



	<!-- 1. INFECTIOUS DISEASE SCENARIO type -->
	<!-- -->

	<complexType name="InfectiousDiseaseScenario">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="scenarioDate" type="dateTime" maxOccurs="1"
						minOccurs="1" />
					<element name="scenarioLocation" type="tns:Location"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="populations" type="tns:Population"
						maxOccurs="unbounded" minOccurs="1" />
					<element name="infections" type="tns:Infection" maxOccurs="unbounded"
						minOccurs="1" />
					<element name="colonizations" type="tns:Colonization"
						maxOccurs="unbounded" minOccurs="0">
					</element>
					<element name="contaminations" type="tns:Contamination"
						maxOccurs="unbounded" minOccurs="0" />

					<element name="contaminatedThingCensuses" type="tns:ContaminatedThingCensus"
						maxOccurs="unbounded" minOccurs="0">
					</element>
					<element name="abioticThingCensuses" type="tns:AbioticThingCensus"
						maxOccurs="unbounded" minOccurs="0">
					</element>
					<element name="infectiousDiseaseControlStrategies" type="tns:InfectiousDiseaseControlMeasure"
						maxOccurs="unbounded" minOccurs="0" />
					<element name="scenarioCartesianOrigin" type="tns:ScenarioCartesianOrigin"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="nonApolloParameters" type="tns:NonApolloParameter"
						maxOccurs="unbounded" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Location">
		<sequence>
			<choice>
				<element name="apolloLocationCode" type="tns:ApolloLocationCode"
					maxOccurs="1" minOccurs="1">
				</element>
				<element name="locationDefinition" type="tns:LocationDefinition"
					maxOccurs="1" minOccurs="1">
				</element>
				<element name="cartesianCircleLocationDefinition" type="tns:CartesianCircleLocationDefinition"
					maxOccurs="1" minOccurs="1">
				</element>
			</choice>
			<element name="textualDescription" type="string" maxOccurs="1"
				minOccurs="0" />
		</sequence>
	</complexType>


	<!-- 2. Seven SIMPLE NUMERIC types that add to those defined by W3C -->
	<!-- -->

	<simpleType name="Probability">
		<restriction base="double">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>

	<simpleType name="Fraction">
		<restriction base="double">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>

	<simpleType name="PositiveDouble">
		<restriction base="double">
			<minExclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="NonNegativeDouble">
		<restriction base="double">
			<minInclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="Percent">
		<restriction base="nonNegativeInteger">
			<minInclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="Latitude">
		<restriction base="double">
			<minInclusive value="-90" />
			<maxInclusive value="90" />
		</restriction>
	</simpleType>

	<simpleType name="Longitude">
		<restriction base="double">
			<minInclusive value="-180" />
			<maxInclusive value="180" />
		</restriction>
	</simpleType>


	<!-- 3. Types for representing a MULTIDIMENSIONAL ARRAY over categorical 
		variables as a tree (recursive) -->
	<!-- -->

	<complexType name="VariableCategoryValueTree">
		<sequence>
			<element name="nVariables" type="positiveInteger" maxOccurs="1"
				minOccurs="0">
			</element>
			<element name="rootVariableNode" type="tns:CategoricalVariableNode"
				maxOccurs="1" minOccurs="1" />
		</sequence>
	</complexType>

	<!-- (conditioningVariableEnum s.r. CategoricalVariableEnum) -->
	<complexType name="CategoricalVariableNode">
		<sequence>
			<element name="name" type="tns:CategoricalVariableEnum"
				maxOccurs="1" minOccurs="1" />
			<element name="categories" type="tns:CategoryValueNode"
				maxOccurs="unbounded" minOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="CategoryValueNode">
		<sequence>
			<element name="categoryDefinition" type="tns:CategoryDefinition"
				maxOccurs="1" minOccurs="1" />
			<choice>
				<element name="value" type="nonNegativeInteger" maxOccurs="1"
					minOccurs="1" />
				<element name="nextCategoricalVariable" type="tns:CategoricalVariableNode"
					maxOccurs="1" minOccurs="1" />
			</choice>
		</sequence>
	</complexType>


	<!-- 4. Types for representing DISTANCE DURATION, RATE, and INTERVAL -->
	<!-- -->

	<complexType name="Distance">
		<complexContent>
			<extension base="tns:ParameterValue">
				<sequence>
					<element name="unitOfDistance" type="tns:UnitOfDistanceEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="value" type="double" maxOccurs="1"
						minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="UnitOfDistanceEnum">
		<restriction base="string">
			<enumeration value="meters" />
			<enumeration value="inches" />
			<enumeration value="miles" />
		</restriction>
	</simpleType>

	<complexType name="Duration">
		<complexContent>
			<extension base="tns:ParameterValue">
				<sequence>
					<element name="unitOfTime" type="tns:UnitOfTimeEnum"
						maxOccurs="1" minOccurs="1" />
					<element name="referenceId" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="0" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="UnitOfTimeEnum">
		<restriction base="string">
			<enumeration value="year" />
			<enumeration value="month" />
			<enumeration value="day" />
			<enumeration value="hour" />
			<enumeration value="minute" />
			<enumeration value="second" />
			<enumeration value="millisecond" />
		</restriction>
	</simpleType>

	<complexType name="FixedDuration">
		<complexContent>
			<extension base="tns:Duration">
				<sequence>
					<element name="value" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="UncertainDuration">
		<complexContent>
			<extension base="tns:Duration">
				<sequence>
					<element name="probabilityDistribution" type="tns:ProbabilityDistribution"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Rate">
		<sequence>
			<element name="numeratorUnitOfMeasure" type="tns:UnitOfMeasureEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="denominatorUnitOfMeasure" type="tns:UnitOfMeasureEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<choice>
				<element name="value" type="double" maxOccurs="1" minOccurs="1">
				</element>
				<element name="probabilityDistribution" type="tns:ProbabilityDistribution"
					maxOccurs="1" minOccurs="1">
				</element>
			</choice>
		</sequence>
	</complexType>

	<complexType name="Interval">
		<sequence>
			<element name="startBoundaryDefinition" type="tns:IntervalBoundaryDefinitionEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="endBoundaryDefinition" type="tns:IntervalBoundaryDefinitionEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="duration" type="tns:Duration" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="readableTitle" type="token" maxOccurs="1"
				minOccurs="0">
			</element>
			<element name="referenceId" type="nonNegativeInteger"
				maxOccurs="1" minOccurs="0">
			</element>
		</sequence>
	</complexType>

	<simpleType name="IntervalBoundaryDefinitionEnum">
		<restriction base="string">
			<enumeration value="timeOfFirstSymptom" />
			<enumeration value="timeOfRecovery" />
			<enumeration value="timeOfHospitalAdmission" />
			<enumeration value="timeOfHospitalDischarge" />
			<enumeration value="timeOfDeath" />
			<enumeration value="timeOfWhoNotification" />
			<enumeration value="timeOfCaseDetection" />
		</restriction>
	</simpleType>



	<!-- 5. Types for representing uncertain values as PROBABILITY DISTRIBUTIONS -->
	<!-- -->

	<complexType name="UncertainValue" />

	<!-- a type hierarchy for probability distributions -->

	<complexType name="ProbabilityDistribution">
		<complexContent>
			<extension base="tns:UncertainValue">
				<sequence>
					<element name="referenceId" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="0" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="UnconditionalProbabilityDistribution">
		<complexContent>
			<extension base="tns:ProbabilityDistribution" />
		</complexContent>
	</complexType>

	<complexType name="NonparametricProbabilityDistribution">
		<complexContent>
			<extension base="tns:UnconditionalProbabilityDistribution" />
		</complexContent>
	</complexType>

	<complexType name="ParametricProbabilityDistribution">
		<complexContent>
			<extension base="tns:UnconditionalProbabilityDistribution" />
		</complexContent>
	</complexType>

	<complexType name="DiscreteParametricProbabilityDistribution">
		<complexContent>
			<extension base="tns:ParametricProbabilityDistribution" />
		</complexContent>
	</complexType>

	<complexType name="DiscreteNonparametricProbabilityDistribution">
		<complexContent>
			<extension base="tns:NonparametricProbabilityDistribution">
				<sequence>
					<element name="probabilityValuePairs" type="tns:ProbabilityValuePair"
						maxOccurs="unbounded" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ContinuousParametricProbabilityDistribution">
		<complexContent>
			<extension base="tns:ParametricProbabilityDistribution" />
		</complexContent>
	</complexType>

	<!-- specific probability distributions... -->

	<complexType name="LogNormalDistribution">
		<complexContent>
			<extension base="tns:ContinuousParametricProbabilityDistribution">
				<sequence>
					<element name="mean" type="double" maxOccurs="1" minOccurs="1" />
					<element name="standardDeviation" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="shiftRight" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="0" />
					<element name="cutTailAt" type="tns:PositiveDouble"
						maxOccurs="1" minOccurs="0" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="NormalDistribution">
		<complexContent>
			<extension base="tns:ContinuousParametricProbabilityDistribution">
				<sequence>
					<element name="mean" type="double" maxOccurs="1" minOccurs="1" />
					<element name="standardDeviation" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="shiftRight" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="0" />
					<element name="cutTailAt" type="tns:PositiveDouble"
						maxOccurs="1" minOccurs="0" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GammaDistribution">
		<complexContent>
			<extension base="tns:ContinuousParametricProbabilityDistribution">
				<sequence>
					<element name="k-shape" type="tns:PositiveDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="theta-scale" type="tns:PositiveDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="shiftRight" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="cutTailAt" type="tns:PositiveDouble"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="WeibullDistribution">
		<complexContent>
			<extension base="tns:ContinuousParametricProbabilityDistribution">
				<sequence>
					<element name="k-shape" type="tns:PositiveDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="lambda-scale" type="tns:PositiveDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="shiftRight" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="cutTailAt" type="tns:PositiveDouble"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ContinuousUniformDistribution">
		<complexContent>
			<extension base="tns:ContinuousParametricProbabilityDistribution">
				<sequence>
					<element name="minimumValue" type="double" maxOccurs="1"
						minOccurs="1" />
					<element name="maximumValue" type="double" maxOccurs="1"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ConditionalProbabilityDistribution">
		<complexContent>
			<extension base="tns:ProbabilityDistribution">
				<sequence>
					<element name="firstConditioningVariable" type="tns:ConditioningVariable"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>



	<!-- 6. types for representing summary statistics -->
	<!-- -->

	<complexType name="MeanWithConfidenceInterval">
		<complexContent>
			<extension base="tns:UncertainValue">
				<sequence>
					<element name="mean" type="double" maxOccurs="1" minOccurs="1" />
					<element name="lowerBound" type="double" maxOccurs="1"
						minOccurs="1" />
					<element name="upperBound" type="double" maxOccurs="1"
						minOccurs="1" />
					<element name="confidence" type="tns:Percent" maxOccurs="1"
						minOccurs="1">
					</element>
					<element name="sampleSize" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="MeanWithStandardDeviation">
		<complexContent>
			<extension base="tns:UncertainValue">
				<sequence>
					<element name="mean" type="double" maxOccurs="1" minOccurs="1" />
					<element name="standardDeviation" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="sampleSize" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="MeanMedianMinimumMaximum">
		<complexContent>
			<extension base="tns:UncertainValue">
				<sequence>
					<element name="mean" type="double" maxOccurs="1" minOccurs="0" />
					<element name="median" type="double" maxOccurs="1"
						minOccurs="0" />
					<element name="minimumValue" type="double" maxOccurs="1"
						minOccurs="1" />
					<element name="maximumValue" type="double" maxOccurs="1"
						minOccurs="1" />
					<element name="sampleSize" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<!-- 7. Simple types for identifiers for species, drugs, vaccines... -->
	<!-- -->

	<simpleType name="NcbiTaxonId">
		<restriction base="token">
			<pattern value="[0-9]"></pattern>
		</restriction>
	</simpleType>

	<complexType name="ApolloPathogenCode">
		<sequence>
			<element name="ncbiTaxonId" type="tns:NcbiTaxonId" maxOccurs="1"
				minOccurs="1" />
			<element name="cladeName" type="token" maxOccurs="1"
				minOccurs="0" />
		</sequence>
	</complexType>

	<!-- for diseases (and results of tests?***) -->

	<simpleType name="SnomedId">
		<restriction base="token">
			<pattern value="[0-9]"></pattern>
		</restriction>
	</simpleType>

	<!-- for vaccines -->

	<simpleType name="vaccineOntologyId">
		<restriction base="token">
			<pattern value="[A-Z_0-9]{2,17}"></pattern>
		</restriction>
	</simpleType>

	<!-- for drugs -->

	<simpleType name="RxNormId">
		<restriction base="token">
			<pattern value="[0-9]"></pattern>
		</restriction>
	</simpleType>

	<!-- for laboratory tests -->

	<simpleType name="LoincId">
		<restriction base="string">
			<pattern value="[0-9]{5}-[0-9]{1}"></pattern>
		</restriction>
	</simpleType>

	<simpleType name="ApolloLocationCode">
		<restriction base="token">
			<pattern value="[A-Z0-9]{2,17}"></pattern>
		</restriction>
	</simpleType>

	<!-- X. Types for representing a population (only used for human pops at present*** -->

	<complexType name="Population">
		<sequence>
			<element name="location" type="tns:ApolloLocationCode"
				maxOccurs="1" minOccurs="1" />
			<element name="taxonId" type="tns:NcbiTaxonId" maxOccurs="1"
				minOccurs="1" />
			<element name="count" type="positiveInteger" maxOccurs="1"
				minOccurs="1" />
			<element name="infectionAndImmunityCensuses" type="tns:PopulationInfectionAndImmunityCensus"
				maxOccurs="unbounded" minOccurs="0" />
			<element name="colonizationCensuses" type="tns:PopulationColonizationCensus"
				maxOccurs="unbounded" minOccurs="0" />
			<choice>
				<element name="individualsAndHouseholds" type="tns:IndividualsAndHouseholds"
					maxOccurs="1" minOccurs="0" />
				<element name="pointerToIndividualsAndHouseholds" type="tns:PointerToData"
					maxOccurs="1" minOccurs="0" />
			</choice>
			<element name="individualBiologyAndBehavior" type="tns:IndividualBiologyAndBehavior"
				maxOccurs="1" minOccurs="0" />
		</sequence>
	</complexType>

	<!-- X. Population censuses*** -->

	<complexType name="IndividualsAndHouseholds">
		<sequence>
			<element name="individuals" type="tns:Individual" maxOccurs="unbounded"
				minOccurs="1"></element>
			<element name="households" type="tns:Household" maxOccurs="unbounded"
				minOccurs="0"></element>
		</sequence>
	</complexType>

	<!-- X. Individual biology and behavior -->

	<complexType name="IndividualBiologyAndBehavior">
		<sequence>
			<element name="lifeCycle" type="tns:LifeCycle" maxOccurs="1"
				minOccurs="0" />
			<element name="reproduction" type="tns:Reproduction"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="behavior" type="tns:Behavior" maxOccurs="1"
				minOccurs="0">
			</element>
		</sequence>
	</complexType>

	<!-- X. INDIVIDUAL BEHAVIOR AND BIOLOGY types -->

	<complexType name="Reproduction">
	</complexType>

	<complexType name="MosquitoReproduction">
		<complexContent>
			<extension base="tns:Reproduction">
				<sequence>
					<element name="gonotrophicPeriodDuration" type="tns:Duration"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="eggsPerBrood" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="minimumEggsPerOviposition" type="positiveInteger"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="ageDependentFecundityReduction" type="tns:Rate"
						maxOccurs="1" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="LifeCycle">
		<sequence>
			<element name="lifeStagesWithDurationsAndMortalities" type="tns:LifeStageWithDurationAndMortality"
				maxOccurs="unbounded" minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="LifeStageWithDurationAndMortality">
		<sequence>
			<element name="stage" type="tns:DevelopmentalStageEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="duration" type="tns:Duration" maxOccurs="1"
				minOccurs="1">
			</element>
			<choice>
				<element name="mortalityRate" type="tns:Rate" maxOccurs="1"
					minOccurs="1">
				</element>
				<element name="mortalityFunction" type="tns:MortalityFunction"
					maxOccurs="1" minOccurs="1">
				</element>
			</choice>
		</sequence>
	</complexType>

	<complexType name="MortalityFunction"></complexType>

	<complexType name="ClaraDensityDependentMortalityFunction">
		<complexContent>
			<extension base="tns:MortalityFunction">
				<sequence>
					<element name="description" type="string" maxOccurs="1"
						minOccurs="0" />
					<element name="omega" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="1" />
					<element name="sigma" type="tns:NonNegativeDouble"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<simpleType name="DevelopmentalStageEnum">
		<restriction base="string">
			<enumeration value="egg" />
			<enumeration value="larval" />
			<enumeration value="pupal" />
			<enumeration value="larvalAndPupal" />
			<enumeration value="adultForm" />
		</restriction>
	</simpleType>


	<complexType name="Behavior">
	</complexType>

	<complexType name="MosquitoBehavior">
		<complexContent>
			<extension base="tns:Behavior">
				<sequence>
					<element name="biteProbability" type="tns:ProbabilisticParameter"
						maxOccurs="1" minOccurs="0" />
					<element name="biteRadius" type="tns:Distance" maxOccurs="1"
						minOccurs="0" />
					<element name="timeBetweenBites" type="tns:Duration"
						maxOccurs="1" minOccurs="0" />
					<element name="migrationSpeed" type="tns:Rate" maxOccurs="1"
						minOccurs="0" />
					<element name="shadeAffinity" type="tns:Fraction"
						maxOccurs="1" minOccurs="0" />
					<element name="maleMatingRadius" type="tns:Distance"
						maxOccurs="1" minOccurs="0" />
					<element name="matingProbability" type="tns:Probability"
