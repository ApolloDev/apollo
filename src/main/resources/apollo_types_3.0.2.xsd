	</complexType>

	<complexType name="VaccinationEfficacyInferred">
		<complexContent>
			<extension base="tns:TreatmentEfficacy">
				<sequence>
					<element name="inferredFromTheseStudies" type="tns:VaccinationEfficacyStudy"
						maxOccurs="unbounded" minOccurs="1" />
