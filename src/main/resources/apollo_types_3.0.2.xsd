	</simpleType>

	<complexType name="TreatmentSurveillanceTriggerDefinition">
		<complexContent>
			<extension base="tns:TriggerDefinition">
				<sequence>
					<element name="reactiveControlMeasureTest" type="token"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="reactiveControlMeasureThreshold" type="int"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="reactiveControlMeasureOperator" type="tns:OperatorEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="unitOfMeasureForThreshold" type="tns:UnitOfMeasureEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="treatmentSurveillanceCapability" type="tns:TreatmentSurveillanceCapability"
						maxOccurs="1" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<!-- logistical system -->

	<complexType name="LogisticalSystem">
		<sequence>
			<element name="product" type="string" maxOccurs="1"
				minOccurs="1"></element>
			<element name="logisticalSystemNodes" type="tns:LogisticalSystemNode"
				maxOccurs="unbounded" minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="LogisticalSystemNode">
		<sequence>
			<element name="description" type="string" maxOccurs="1"
				minOccurs="0">
			</element>
			<choice>
				<element name="capacitySchedule" type="tns:Schedule"
					minOccurs="1" maxOccurs="1">
				</element>
				<element name="outputSchedule" type="tns:Schedule"
					minOccurs="1" maxOccurs="1">
				</element>
			</choice>
			<element name="children" type="tns:LogisticalSystemNode"
				maxOccurs="unbounded" minOccurs="0"></element>
		</sequence>
	</complexType>

	<complexType name="Schedule">
		<sequence>
			<element name="unitOfMeasure" type="tns:UnitOfMeasureEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="scheduleElements" type="tns:ScheduleElement"
				maxOccurs="unbounded" minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="ScheduleElement">
		<sequence>
			<element name="quantity" type="positiveInteger" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="dateTime" type="dateTime" maxOccurs="1"
				minOccurs="1"></element>
		</sequence>
	</complexType>


	<!-- X. individual treatment control measures -->

	<complexType name="IndividualTreatmentControlMeasure">
		<complexContent>
			<extension base="tns:InfectiousDiseaseControlMeasure">
				<sequence>
					<element name="pathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1">
					</element>
